<h1>Login</h1>

<div class="error"> <%= flash[:login_error] %></div>

<%= form_tag login_path do  %>
    <%= label_tag :username %>
    <%= text_field_tag :username %><br><br>
    <%= label_tag :password %>
    <%= password_field_tag :password%><br><br>
    <%= submit_tag "Log In" %>
<% end %>

<br>
<%= button_to "Create Account", new_user_path, method: :get %>