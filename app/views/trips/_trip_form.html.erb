<%= form_for @trip do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <br>
    <%= f.label :start_date, "Start Date" %>
    <%= f.date_field :start_date, as: :date, value: f.object.try(:strftime, "%m/%d/%Y")%><br>
    <br>
    <%= f.label :end_date, "End Date" %>
    <%= f.date_field :end_date, as: :date, value: f.object.try(:strftime, "%m/%d/%Y")%><br>
    <br>
    <%= f.submit %>
    <% end %>