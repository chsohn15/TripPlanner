<% if @trip_location.errors.any? %>
    <% @trip_location.errors.full_messages.each do |msg| %>
        <%= msg %>
    <% end %>
<% end %>


<%= form_for @trip_location, url:city_location_path(@location), method: :patch do |f| %>
<%= f.label :trip_id %>
<%= f.select :trip_id, options_from_collection_for_select(@trips, :id, :name) %>
<%= f.label :rating %>
<%= f.number_field :rating %>
<%= f.submit "Add to Trip"%>
<% end %>